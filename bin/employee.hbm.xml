<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.codegnan.entity.Employee" table="employee_101">
        <id name="id">
            <generator class="increment"/>
        </id>

        <!-- Example property mapping --><!-- <property name="propertyName" column="column_name" type="data_type"/> -->
        <property name="name"/>
        <property name="email"/>
        <property name="department"/>
        
    </class>
</hibernate-mapping>